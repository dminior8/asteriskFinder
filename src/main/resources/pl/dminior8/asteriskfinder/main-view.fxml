<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="293.0" prefWidth="316.0" spacing="20.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.dminior8.asteriskfinder.controller.JsonController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label fx:id="statusLabel" prefHeight="49.0" prefWidth="274.0" styleClass="status-label" text="Welcome to Asterisk Finder! " />
   <VBox>
      <children>
      
         <Label fx:id="instructionLabel" prefHeight="7.0" prefWidth="274.0" styleClass="instruction-label" text="Enter name of your JSON file" />
         <TextField fx:id="fileNameText" prefHeight="22.0" prefWidth="277.0" />
      </children>
   </VBox>
    <Button fx:id="checkButton" prefHeight="25.0" prefWidth="157.0" styleClass="btn" text="Check" />
   <AnchorPane prefHeight="69.0" prefWidth="276.0">
      <children>
         <Label fx:id="resultLabel" prefHeight="43.0" prefWidth="276.0" text="Asterisk">
            <styleClass>
               <String fx:value="result-label" />
               <String fx:value="alert" />
               <String fx:value="alert-success" />
            </styleClass></Label>
      </children>
   </AnchorPane>
</VBox>
