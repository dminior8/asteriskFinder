<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="293.0" prefWidth="316.0" spacing="20.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.dminior8.asteriskfinder.controller.JsonController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label fx:id="statusLabel" prefHeight="49.0" prefWidth="274.0" text="Welcome to Asterisk Finder! " />

   <Label fx:id="instructionLabel" prefHeight="17.0" prefWidth="274.0" text="Enter name of your JSON file" />
   <TextField fx:id="fileNameText" />
    <Button fx:id="checkButton" prefHeight="25.0" prefWidth="157.0" text="Check" />
   <AnchorPane prefHeight="69.0" prefWidth="276.0">
      <children>
         <Label fx:id="resultLabel" prefHeight="63.0" prefWidth="274.0" text="Asterisk" />
      </children>
   </AnchorPane>
</VBox>
